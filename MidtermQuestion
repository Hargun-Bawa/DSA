#include <iostream>
#include <string>
using namespace std;

class PostfixExpression{
private:
    string expression;
    int index;
public:
    PostfixExpression(string);
    string nextToken();
    int length();
};
PostfixExpression::PostfixExpression(string exp){
    expression = exp;
    index = 0;
}
string PostfixExpression::nextToken(){
    return expression.substr(index++,1);
}
int PostfixExpression::length(){
    return expression.length();
}
int main(){
    PostfixExpression a("12467");
    cout << a.nextToken() << endl;
    cout << a.nextToken() << endl;
    cout << a.length() << endl;
}
